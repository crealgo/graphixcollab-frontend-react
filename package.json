  "scripts": {
    "dev": "concurrently npm:site:dev npm:tokens:dev",
    "build": "rollup -c",
    "verify": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --fix --ext .ts,.tsx src/",
    "docs:dev": "storybook dev -p 6006 -s public",
    "docs:build": "storybook build -s public",
    "docs:serve": "npx http-server storybook-static -p 6006",
    "create:tag": "node scripts/createTag",
    "create:service": "node scripts/createService",
    "create:timeline": "node scripts/createTimeline",
    "site:dev": "next dev",
    "site:build": "rm -rf .next && next build && next export",
    "site:serve": "npx http-server out -p 6006",
    "scripts:dev": "npm-watch scripts:rename",
    "scripts:clean": "rm -rf lib .next dist storybook-static",
    "scripts:entry-file": "node scripts/generateEntryFile",
    "tokens:dev": "npm-watch tokens:build",
    "tokens:build": "node tokens/scripts/buildTokens.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@icons-pack/react-simple-icons": "^7.1.1",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
