{
  "name": "@crealgo/hydra-react",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/inter-tight": "^5.0.2",
    "@hookform/resolvers": "^3.1.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.10.11",
    "@react-spring/web": "^9.7.2",
    "@svgr/webpack": "^6.5.1",
    "@types/gh-pages": "^3.2.1",
    "@types/prompt-sync": "^4.2.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "code-tag": "^1.1.0",
    "colord": "^2.9.3",
    "embla-carousel-autoplay": "^8.0.0-rc01",
    "embla-carousel-react": "^8.0.0-rc01",
    "gh-pages": "^5.0.0",
    "googleapis": "^118.0.0",
    "next": "^13.1.6",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-hook-form": "^7.44.3",
    "storybook": "^7.0.21",
    "tsx": "^3.12.7",
    "typescript-plugin-css-modules": "^5.0.1",
    "yaml": "^2.2.1",
    "yup": "^1.2.0"
  },
  "scripts": {
    "dev": "concurrently npm:site:dev npm:tokens:dev npm:script:resize-images-watch",
    "verify": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --fix --ext .ts,.tsx src/",
    "test:lighthouse": "lighthouse https://graphixcollab.test --output-path=lighthouse.html && mv lighthouse.html ~/Developer/Sites/graphixcollab/lighthouse.html",
    "test:playwright": "playwright test",
    "docs:dev": "storybook dev -p 6006 -s public",
    "docs:build": "storybook build -s public",
    "docs:serve": "npx http-server storybook-static -p 6006",
    "create:tag": "node scripts/createTag",
    "create:service": "node scripts/createService",
    "create:timeline": "node scripts/createTimeline",
    "site:dev": "next dev",
    "site:build": "next build && next export",
    "site:serve": "npx http-server out -p 6006",
    "presite:deploy-dev": "rm -rf .next && env-cmd -f .env.development npm run site:build",
    "site:deploy-dev": "npx gh-pages -d out --repo https://github.com/crealgo/crealgo-static-projects --dest ./graphixcollab",
    "presite:deploy-staging": "rm -rf .next && env-cmd -f .env.staging npm run site:build",
    "site:deploy-staging": "npx gh-pages -d out --repo https://github.com/crealgo/graphixcollab-static-staging",
    "script:resize-images-watch": "nodemon --watch src --ext '*' --exec \"npm run script:resize-images\"",
    "script:resize-images": "node scripts/resizeImages",
    "script:dev": "npm-watch script:rename",
    "script:clean": "rm -rf lib .next dist storybook-static",
    "script:entry-file": "node scripts/generateEntryFile",
    "tokens:dev": "nodemon --watch tokens/src --ext ts --exec \"npm run tokens:build\"",
    "tokens:build": "tsx tokens/src",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@icons-pack/react-simple-icons": "^7.1.1",
    "@playwright/experimental-ct-react": "^1.35.1",
    "@playwright/test": "^1.34.3",
    "@storybook/addon-a11y": "^7.0.21",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-mdx-gfm": "^7.0.21",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.0.21",
    "@storybook/react-vite": "^7.0.21",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/types": "^7.0.21",
    "@types/chance": "^1.1.3",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "chalk": "^5.2.0",
    "chance": "^1.1.9",
    "change-case": "^4.1.2",
    "concurrently": "^8.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-yaml": "^0.5.0",
    "glob": "^9.2.1",
    "lighthouse": "^10.2.0",
    "mkdirp": "^3.0.1",
    "nodemon": "^2.0.22",
    "playwright": "^1.34.3",
    "prettier": "^2.8.8",
    "prompt-sync": "^4.2.0",
    "sharp": "^0.32.1",
    "style-dictionary": "^3.7.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.31.1"
  }
}
