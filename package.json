{
  "name": "@crealgo/hydra-react",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@emotion/css": "^11.10.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.15",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.11",
    "@react-spring/web": "^9.7.2",
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "clsx": "^1.2.1",
    "code-tag": "^1.1.0",
    "colord": "^2.9.3",
    "embla-carousel-autoplay": "^8.0.0-rc01",
    "embla-carousel-react": "^8.0.0-rc01",
    "gh-pages": "^5.0.0",
    "next": "^13.1.6",
    "npm-watch": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "storybook": "^7.0.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "yaml": "^2.2.1"
  },
  "scripts": {
    "dev": "concurrently npm:site:dev npm:tokens:dev",
    "build": "rollup -c",
    "verify": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --fix --ext .ts,.tsx src/",
    "test:lighthouse": "lighthouse https://graphixcollab.test --output-path=lighthouse.html && mv lighthouse.html ~/Developer/Sites/graphixcollab/lighthouse.html",
    "docs:dev": "storybook dev -p 6006 -s public",
    "docs:build": "storybook build -s public",
    "docs:serve": "npx http-server storybook-static -p 6006",
    "create:tag": "node scripts/createTag",
    "create:service": "node scripts/createService",
    "create:timeline": "node scripts/createTimeline",
    "site:dev": "next dev",
    "site:build": "rm -rf .next && next build && next export",
    "site:serve": "npx http-server out -p 6006",
    "presite:deploy-local": "npm run site:build",
    "site:deploy-local": "rm -rf ~/Developer/Sites/graphixcollab && mv out ~/Developer/Sites/graphixcollab",
    "presite:deploy-dev": "npm run site:build",
    "site:deploy-dev": "npx gh-pages -d out --repo https://github.com/crealgo/crealgo-static-projects --dest ./graphixcollab",
    "presite:deploy-prod": "npm run site:build && echo beta.graphixcollab.com > ./out/CNAME",
    "site:deploy-prod": "npx gh-pages -d out --repo https://github.com/crealgo/graphixcollab-static",
    "scripts:regenerate-images": "node scripts/resizeImages",
    "scripts:dev": "npm-watch scripts:rename",
    "scripts:clean": "rm -rf lib .next dist storybook-static",
    "scripts:entry-file": "node scripts/generateEntryFile",
    "tokens:dev": "npm-watch tokens:build",
    "tokens:build": "node tokens/scripts/buildTokens.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@icons-pack/react-simple-icons": "^7.1.1",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/types": "^7.0.18",
    "@types/chance": "^1.1.3",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "chalk": "^4.1.2",
    "chance": "^1.1.9",
    "change-case": "^4.1.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-yaml": "^0.5.0",
    "glob": "^9.2.1",
    "lighthouse": "^10.2.0",
    "playwright": "^1.34.3",
    "prettier": "^2.8.8",
    "prompt-sync": "^4.2.0",
    "sharp": "^0.32.1",
    "style-dictionary": "^3.7.2",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.31.1"
  },
  "watch": {
    "scripts:rename": "scripts/changeFilenameCase.ts",
    "tokens:build": "tokens/{src,scripts}/**/*.js"
  }
}
