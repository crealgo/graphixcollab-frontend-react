    "ts-dedent": "^2.2.0",
    "web-vitals": "^2.1.4",
    "yaml": "^2.2.1"
  },
  "scripts": {
    "dev": "npm run site:dev",
    "build": "rollup -c",
    "verify": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint --fix --ext .ts,.tsx src/",
    "docs:dev": "storybook dev -p 6006 -s public",
    "docs:build": "storybook build -s public",
    "site:dev": "next dev",
    "site:build": "rm -rf .next && next build && next export",
    "scripts:dev": "npm-watch scripts:rename",
    "scripts:clean": "rm -rf lib .next dist storybook-static",
    "scripts:rename": "ts-node scripts/changeFilenameCase",
    "scripts:entry-file": "ts-node scripts/generateEntryFile",
    "tokens:dev": "npm-watch tokens:build",
    "tokens:build": "style-dictionary build -c tokens/config.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@icons-pack/react-simple-icons": "^7.1.1",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^7.0.2",
    "@styled/typescript-styled-plugin": "^0.20.0",
    "@trunkio/launcher": "^1.2.4",
    "@types/chance": "^1.1.3",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chance": "^1.1.9",
    "change-case": "^4.1.2",
    "eslint": "^8.38.0",
    "eslint-plugin-editorconfig": "^4.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.11",
    "glob": "^9.2.1",
    "prettier": "^2.8.7",
    "puppeteer": "^19.4.1",
    "style-dictionary": "^3.7.2",
    "tailwindcss": "^3.3.1",
    "trunk": "^1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "watch": {
    "scripts:rename": "scripts/changeFilenameCase.ts",
    "tokens:build": "tokens/config.js"
  }
}
