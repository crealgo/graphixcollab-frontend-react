    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "clsx": "^1.2.1",
    "colord": "^2.9.3",
    "css-to-object": "^1.1.0",
    "eslint-config-prettier": "^8.8.0",
    "gh-pages": "^5.0.0",
    "next": "^13.1.6",
    "npm-watch": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
